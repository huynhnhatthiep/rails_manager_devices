  <div class="container-fluid mt--8">
    <div class="row" style="tex">
      <div class="col-xl-1 order-xl-1"></div>
      <div class="col-xl-10 order-xl-1">
        <div class="card">
          <div class="card-header">
            <div class="row align-items-center">
              <div class="col-8">
                <h3 class="mb-0">Profile </h3>
              </div>
            </div>
          </div>
          <div class="card-body">
            <div style=" text-align: center;">
                <%flash.each do |name, msg|%>
                  <span class = 'errors'> <%= msg %></span>
                <% end %>
            </div>
            <%= form_for @user, url: {action: :update} do |f| %>
              <% if @user.errors.any? %>
                <ul class = 'item_errors'>
                  <% @user.errors.full_messages.each do |message| %>
                  <li class = 'errors'><%= message %></li>
                  <% end %>
                </ul>
              <% end %>
              <h6 class="heading-small text-muted mb-4">Contact information</h6>
              
              <div class="pl-lg-4">
                <div class="row">
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label" for="input-username">Username</label>
                        <%= f.text_field :name, :class => 'form-control' %>   
                      </div>
                    </div>
                    <div class="col-lg-6">
                      <div class="form-group">
                        <label class="form-control-label" for="input-email">Email address</label>
                        <%= f.text_field :email, :class => 'form-control' %>
                      </div>
                    </div>
                  </div>
                <div class="row">
                  <div class="col-lg-6">
                    <div class="form-group">
                      <label class="form-control-label" for="input-first-name">Address</label>
                      <%= f.text_field :address, :class => 'form-control' %>
                    </div> 
                  </div>
                  <div class="col-lg-6">
                    <div class="form-group">
                      <label class="form-control-label" for="input-first-name">phone</label>
                      <%= f.text_field :phone_number, :class => 'form-control' %>
                    </div>
                  </div>
                </div>
              </div>
              <hr class="my-4" />
              
              <div class="pl-lg-4" style = "text-align: center">
                  <%= f.submit :class => 'btn btn-primary btn-submit'%>
              </div>
            <%end%>
          </div>
        </div>
      </div>
    </div>
